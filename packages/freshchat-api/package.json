{
  "name": "@freshworks-jaya/freshchat-api",
  "version": "0.7.36-beta-01",
  "description": "Provides simple interface for accessing Freshchat's public APIs",
  "repository": "git@github.com:freshdesk/jaya-lib.git",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rm -rf lib coverage .nyc_output",
    "watch": "tsc --watch --incremental",
    "coverage": "codecov",
    "test": "nyc --reporter=lcov mocha",
    "build": "tsc && cpx \"src/*.hbs\" ./lib",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:check": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint 'src/**/*.ts' --quiet --fix",
    "prepare": "npm run build",
    "preversion": "npm run lint"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/handlebars-helpers": "^0.5.6",
    "@types/html-to-text": "^9.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "codecov": "^3.8.3",
    "cpx": "^1.5.0",
    "mocha": "^10.2.0",
    "nock": "^13.4.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@freshworks-jaya/utilities": "^1.0.1-beta-01",
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "handlebars": "^4.7.8",
    "handlebars-helpers": "^0.10.0",
    "html-to-text": "^9.0.5"
  },
  "resolutions": {
    "handlebars": "4.7.7"
  }
}
